{
    "name": "isucon9-qualify/isucari",
    "description": "A sample app for Isucon9",
    "keywords": ["isucon", "isucon9"],
    "homepage": "http://isucon.net/",
    "license": "MIT",
    "require": {
        "php": "~8.0",
        "ext-pdo": "*",
        "bryanjhv/slim-session": "*",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "^1.17",
        "php-di/php-di": "^6.4",
        "slim/php-view": "^2.0",
        "slim/psr7": "^1.5",
        "slim/slim": "^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S 0.0.0.0:8000 -t public public/index.php"
    }
}
